{
	"AllowedHosts": "*",
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning"
			}
		},
		"Enrich": [
			"FromLogContext",
			"WithExceptionDetails"
		],
		"WriteTo": [
			{
				"Name": "Console"
			},
			{
				"Name": "MSSqlServer",
				"Args": {
					"connectionString": "I don't even know. Just give me the data, okay?",
					"schemaName": "log",
					"tableName": "AppLog",
					"autoCreateSqlTable": false,
					"restrictedToMinimumLevel": "Information",
					"batchPostingLimit": 10,
					"period": "0.00:00:05",
					"columnOptionsSection": {
						"Id": {
							"columnName": "AppLogId"
						},
						"level": {
							"storeAsEnum": true
						},
						"additionalColumns": [
							{
								"ColumnName": "Category",
								"DataType": "nvarchar",
								"DataLength": 32
							},
							{
								"ColumnName": "Application",
								"DataType": "nvarchar",
								"DataLength": 32
							}
						]
					}
				}
			},
			{
				"Name": "EventLog",
				"Args": {
					"source": "CoreXplore App",
					"logName": "CoreXplore",
					"restrictedToMinimumLevel": "Information"
				}
			}
		]
	},
	"SerilogEmail": {
		"Enabled": false,
		"From": "",
		"To": "",
		"Server": "",
		"Username": "",
		"Password": "",
		"EnableSsl": true,
		"Port": 465
	},
	"FriendlyDbNames": {
		"Local": "SQLEXPRESS",
		"Production": "windows"
	}
}
